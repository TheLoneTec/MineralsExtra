        <MarketValue>1.0</MarketValue>
        <MaxHitPoints>1.1</MaxHitPoints>
        <WorkToBuild>5.0</WorkToBuild>
        <WorkToMake>4.0</WorkToMake>
        <Mass>1.2</Mass>
